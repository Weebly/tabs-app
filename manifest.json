{
	"manifest": "1",
	"version": "1.0.0",
	"locale": {
		"default": "en-us",
		"supported": ["en-us"]
	},
	"elements": [
		{
			"name": "Tabs",
			"path": "files",
			"version": "1.0.0",
			"settings": {
				"config": {},
				"properties": [
					{
						"type": "group",
						"name": "settings",
						"label": "Settings",
						"properties": [
							{
								"name": "tabs",
								"label": "Number of Tabs",
								"type": "int",
								"default": 2,
								"min": "1",
								"max": "10"
							},
							{
								"name": "tab_width",
								"label": "Tab Width (%)",
								"type": "int",
								"default": 20,
								"min": "1",
								"max": "100"
							},
							{
								"name": "theme",
								"label": "Theme",
								"type": "select",
								"default": "standard",
								"values": [
									{"name":"Standard", "value": "standard"},
									{"name":"Line", "value": "line"},
									{"name":"3D", "value": "s3d"},
									{"name":"Simple", "value": "simple"}
		                        ]
							}
						]
					}
				]
			}
		}
	]
}
